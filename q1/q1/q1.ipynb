    "length = int(input(\"length\"))\n",
    "count=0\n",
    "num_arr=[]\n",
    "\n",
    "for i in range(length):\n",
    "    num_arr.append(int(input(\"Enter number \"+str(i+1)+ \": \")))\n",
    "\n",
    "for i in range(length-1):\n",
    "    if num_arr[i]%2==0 and num_arr[i+1]%2==0:\n",
    "        count=count+1\n",
    "print(count)"
